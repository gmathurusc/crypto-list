<div class="range-filter">
    <button class="btn btn-default btn-group" id="range-filter-btn" type="button" title="Range Filters" data-toggle="modal" data-target="#range-filter">
        <i class="glyphicon glyphicon-filter"></i>
    </button>
</div>

<!-- Modal -->
<div class="modal fade" id="range-filter" tabindex="-1" role="dialog" aria-labelledby="filter-range-title" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content" style="width: 120% !important;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </button>
                <h4 class="modal-title">Filter Range</h4>
            </div>
            <div class="modal-body" style="text-align: left;">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-3"><label style="color: black"> Min Price : </label> </div>
                        <div class="col-md-3"><input type="text" id="price_min"> </div>
                        <div class="col-md-3"><label style="color: black"> Max Price : </label> </div>
                        <div class="col-md-3"><input type="text" id="price_max"> </div>
                    </div>
                    <div class="row" style="margin-top: 5px;">
                        <div class="col-md-3"><label style="color: black"> Mix Max Supply : </label> </div>
                        <div class="col-md-3"><input type="text" id="min_max_supply"></div>
                        <div class="col-md-3"><label style="color: black"> Max Max Supply : </label></div>
                        <div class="col-md-3"><input type="text" id="max_max_supply"></div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="filter-range-clear-btn" data-dismiss="modal" class="btn btn-default">Clear</button>
                <button type="button" id="filter-range-save-btn" data-dismiss="modal" class="btn btn-primary">Filter</button>
            </div>
        </div>
    </div>
</div>


<script>
    var jsonData =  (<%-JSON.stringify(tickers)%>);

    $( "#filter-range-save-btn" ).click(function() {
        rangeFilter(jsonData);
    });

    $( "#filter-range-clear-btn" ).click(function() {
        clearRangeFilter();
    });

    function clearRangeFilter() {
        $("#range-filter :input").each(function(){
            var input = $(this);
            if(input.val() != undefined && input.val().length > 0) {
                input.val('');
            }
        });
        rangeFilter(jsonData);
    }
</script>
